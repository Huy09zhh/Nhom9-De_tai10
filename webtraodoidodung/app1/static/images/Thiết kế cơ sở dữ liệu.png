erd_updated = Digraph(<span class="hljs-string">'ERD_Updated'</span>, filename=<span class="hljs-string">'database_erd_updated'</span>, <span class="hljs-built_in">format</span>=<span class="hljs-string">'png'</span>)

<span class="hljs-comment"># Cấu hình hiển thị</span>
erd_updated.attr(rankdir=<span class="hljs-string">'LR'</span>, size=<span class="hljs-string">'10'</span>)

<span class="hljs-comment"># Bảng User (Cha của các bảng khác)</span>
erd_updated.node(<span class="hljs-string">'User'</span>, <span class="hljs-string">'User\n(user_id PK, username, email, password, created_at, role)'</span>)

<span class="hljs-comment"># Các bảng kế thừa từ User</span>
erd_updated.node(<span class="hljs-string">'Guest'</span>, <span class="hljs-string">'Guest\n(guest_id PK, access_level)'</span>)
erd_updated.node(<span class="hljs-string">'Seller'</span>, <span class="hljs-string">'Seller\n(seller_id PK, store_name, rating, transaction_history)'</span>)
erd_updated.node(<span class="hljs-string">'Buyer'</span>, <span class="hljs-string">'Buyer\n(buyer_id PK, address, search_history, wishlist)'</span>)
erd_updated.node(<span class="hljs-string">'Administrator'</span>, <span class="hljs-string">'Administrator\n(admin_id PK, permissions)'</span>)
erd_updated.node(<span class="hljs-string">'Moderator'</span>, <span class="hljs-string">'Moderator\n(moderator_id PK, moderation_area, violation_reports)'</span>)

<span class="hljs-comment"># Bảng Product</span>
erd_updated.node(<span class="hljs-string">'Product'</span>, <span class="hljs-string">'Product\n(product_id PK, seller_id FK, category_id FK, name, description, price, stock_quantity, created_at)'</span>)

<span class="hljs-comment"># Bảng Orders và Order_Detail</span>
erd_updated.node(<span class="hljs-string">'Orders'</span>, <span class="hljs-string">'Orders\n(order_id PK, buyer_id FK, total_price, order_date, status)'</span>)
erd_updated.node(<span class="hljs-string">'Order_Detail'</span>, <span class="hljs-string">'Order_Detail\n(order_detail_id PK, order_id FK, product_id FK, quantity, price_at_purchase)'</span>)

<span class="hljs-comment"># Bảng Views</span>
erd_updated.node(<span class="hljs-string">'Views'</span>, <span class="hljs-string">'Views\n(view_id PK, user_id FK, product_id FK, viewed_at)'</span>)

<span class="hljs-comment"># Bảng Category</span>
erd_updated.node(<span class="hljs-string">'Category'</span>, <span class="hljs-string">'Category\n(category_id PK, name, parent_category_id FK)'</span>)

<span class="hljs-comment"># Bảng Comments &amp; Ratings</span>
erd_updated.node(<span class="hljs-string">'Comments'</span>, <span class="hljs-string">'Comments\n(comment_id PK, user_id FK, product_id FK, content, created_at)'</span>)
erd_updated.node(<span class="hljs-string">'Ratings'</span>, <span class="hljs-string">'Ratings\n(rating_id PK, user_id FK, product_id FK, rating_value, created_at)'</span>)

<span class="hljs-comment"># Bảng Violation Reports</span>
erd_updated.node(<span class="hljs-string">'Violation_Reports'</span>, <span class="hljs-string">'Violation_Reports\n(report_id PK, reporter_id FK, reported_user_id FK, reason, status, reviewed_by FK)'</span>)

<span class="hljs-comment"># Quan hệ User với các bảng con</span>
erd_updated.edge(<span class="hljs-string">'User'</span>, <span class="hljs-string">'Guest'</span>, label=<span class="hljs-string">'1 - 1'</span>)
erd_updated.edge(<span class="hljs-string">'User'</span>, <span class="hljs-string">'Seller'</span>, label=<span class="hljs-string">'1 - 1'</span>)
erd_updated.edge(<span class="hljs-string">'User'</span>, <span class="hljs-string">'Buyer'</span>, label=<span class="hljs-string">'1 - 1'</span>)
erd_updated.edge(<span class="hljs-string">'User'</span>, <span class="hljs-string">'Administrator'</span>, label=<span class="hljs-string">'1 - 1'</span>)
erd_updated.edge(<span class="hljs-string">'User'</span>, <span class="hljs-string">'Moderator'</span>, label=<span class="hljs-string">'1 - 1'</span>)

<span class="hljs-comment"># Quan hệ Seller - Product</span>
erd_updated.edge(<span class="hljs-string">'Seller'</span>, <span class="hljs-string">'Product'</span>, label=<span class="hljs-string">'1 - N'</span>)

<span class="hljs-comment"># Quan hệ Buyer - Orders</span>
erd_updated.edge(<span class="hljs-string">'Buyer'</span>, <span class="hljs-string">'Orders'</span>, label=<span class="hljs-string">'1 - N'</span>)

<span class="hljs-comment"># Quan hệ Orders - Order_Detail</span>
erd_updated.edge(<span class="hljs-string">'Orders'</span>, <span class="hljs-string">'Order_Detail'</span>, label=<span class="hljs-string">'1 - N'</span>)

<span class="hljs-comment"># Quan hệ Product - Order_Detail</span>
